<div id="works">

<% data.projects.project01.each_with_index do |project, i| %>
  <section class="work work-right wow fadeIn" data-wow-delay=".0s" data-wow-offset="50">

    <div class="row">

      <a href=<%= project.url %> class="work-info col-xs-12 col-md-3 off-md-3">
        <div class="caption">
          <span>
            <h3><%= project.title %></h3>
            <ul>
              <% project.tags.each do |f| %>
              <li><%= f %></li>
              <% end %>
            </ul>
          </span>
        </div>
      </a>

      <a href=<%= project.url %> class="work-visual col-xs-12 col-md-6">
        <span class="work-logo"><%= image_tag project.logo, width:'140', alt: project.title %></span>
        <%= image_tag project.image, alt: project.title %>
      </a>

    </div>

    <div class="category wow fadeIn" data-wow-delay=".0s" data-wow-offset="50">
      <%= project.category %>
    </div>

  </section>
<% end %>

